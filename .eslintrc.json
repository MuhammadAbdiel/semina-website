{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/newline-after-import": 0,
        "import/no-extraneous-dependencies": 0,
        "no-use-before-define": 0,
        "no-console": 0,
        "consistent-return": 0,
        "object-curly-newline": 0,
        "no-useless-constructor": 0,
        "max-len": 0,
        "no-unused-vars": 0,
        "object-shorthand": 0,
        "func-names": 0
    }
}